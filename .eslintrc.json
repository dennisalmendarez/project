{
  "extends": ["eslint:recommended", "prettier"],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": 1,
    "quotes": ["error", "double", { "allowTemplateLiterals": true }]
  }
}
